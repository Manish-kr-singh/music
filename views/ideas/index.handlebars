{{#each ideas}}
  <div class="card card-body mb-2">
    <h4>{{name}}</h4>
    <p>{{years_active}}</p>
    <p>{{description}}</p>
    <p>{{album}}</p>
    <p>{{releaseDate}}</p>
    <a class="btn btn-dark btn-block" href="/ideas/edit/{{id}}">Edit</a>
  </div>
{{else}}
  <p>No Artist listed</p>
{{/each}}
